import java.io.*;
public class Tr8_8 {
	public static void main(String[] args) {
		String txt = "c:\\temp\\phone.txt";
		
		try {
			BufferedReader bin = new BufferedReader(new InputStreamReader(System.in)); // 선언
			String s = bin.readLine(); // String
			int c = Integer.parseInt(bin.readLine());
			while ((c = bin.read()) != -1) {
				System.out.print((char)c);
			}			
			bin.close();
		}
		catch (IOException e) {
			System.out.println("입출력 오류");
		}

		System.out.println("YA 20202865 엄지희");
		System.out.println("객체지향프로그래밍 응용 5주차 과제 실습문제 8-2");
	}
}